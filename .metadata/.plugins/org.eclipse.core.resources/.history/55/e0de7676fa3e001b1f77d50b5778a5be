package ejercicios;

public class Ej001 {
	public static void main(String[] args) {
	
		double[] A= new double[3];
		double x;
			for (int i=0; i<A.length; i++) {
				A[i] = i*3;
				System.out.println("A[" +i + "] = " +A[i]);
			}
				
			x = A[0]+A[1]+A[2];
			System.out.println("x = " +x);
	
	}
}
